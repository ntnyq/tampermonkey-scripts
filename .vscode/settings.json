{
  "editor.formatOnSave": true,
  "eslint.experimental.useFlatConfig": true,
  "eslint.options": {
    "overrideConfigFile": "./eslint.config.mjs"
  },
  "eslint.validate": [
    "vue",
    "yaml",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "feedly",
    "filenamify",
    "gmfetch",
    "gmxhr",
    "jszip",
    "pixiv",
    "pixivcat",
    "pximg",
    "Swal",
    "ugoira",
    "xfetch"
  ]
}
